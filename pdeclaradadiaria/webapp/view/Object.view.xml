<mvc:View
	controllerName="com.tasa.pdeclaradadiaria.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:table="sap.ui.table"
	xmlns:semantic="sap.f.semantic">

	<semantic:SemanticPage
		id="page"
		headerPinnable="false"
		toggleHeaderOnTitleClick="false"
		busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">

		<semantic:titleHeading>
			<Title
				text="Detalle de pesca declarada {FECCONMOV}"
				level="H3"/>
		</semantic:titleHeading>

		<semantic:content>
			<IconTabBar
				selectedKey="{objectView>/selectedKey}"
				select="onFilterSelect">
				<content>
					<table:Table
						id="detailTableId"
						selectionMode="None">
						<table:extension>
							<OverflowToolbar>
								<ObjectStatus
									icon="sap-icon://appointment"
									state="Indication06"
									text="{/dateType}"
									title="{/titleTable}"/>
							</OverflowToolbar>
						</table:extension>
						<table:columns>
							<table:Column headerSpan="2,1">
								<table:multiLabels>
									<Label visible="false" />
									<Label text="Centro" />
								</table:multiLabels>
								<table:template>
									<Text text="{oObject/WERKS}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="2,1">
								<table:multiLabels>
									<Label visible="false" />
									<Label text="Planta" />
								</table:multiLabels>
								<table:template>
									<Link
										id="linkTable"
										emphasized="true"
										enabled="true"
										text="{oObject/DESCR}"
										press="onPlantaDetail"/>
								</table:template>
							</table:Column>

							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Total Pesca" />
									<Label text="Decl" />
								</table:multiLabels>
								<table:template>
									<Text text="{oObject/PDLTR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Total Pesca" />
									<Label text="Desc" />
								</table:multiLabels>
								<table:template>
									<Text text="{oObject/PDSTR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Total Pesca" />
									<Label text="Difer %" />
								</table:multiLabels>
								<table:template>
									<Text text="{oObject/PORC_DIFER}" />
								</table:template>
							</table:Column>

							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Propios" />
									<Label text="Decl" />
								</table:multiLabels>
								<table:template>
									<Text text="{PDLPR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Propios" />
									<Label text="Desc" />
								</table:multiLabels>
								<table:template>
									<Text text="{PDSPR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Propios" />
									<Label text="Dif %" />
								</table:multiLabels>
								<table:template>
									<Text text="{}" />
								</table:template>
							</table:Column>

							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Terceros" />
									<Label text="Decl" />
								</table:multiLabels>
								<table:template>
									<Text text="{PDLTR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Terceros" />
									<Label text="Desc" />
								</table:multiLabels>
								<table:template>
									<Text text="{PDSTR}" />
								</table:template>
							</table:Column>
							<table:Column headerSpan="3,1">
								<table:multiLabels>
									<Label text="Pesca Terceros" />
									<Label text="Dif %" />
								</table:multiLabels>
								<table:template>
									<Text text="{}" />
								</table:template>
							</table:Column>
						</table:columns>
					</table:Table>
				</content>
				<items>
					<IconTabFilter
						key="D"
						text="DÃ­a"
						icon="sap-icon://badge">
					</IconTabFilter>
					<IconTabSeparator/>
					<IconTabFilter
						key="R"
						text="Rango"
						icon="sap-icon://initiative"
						iconColor="Positive">
					</IconTabFilter>
				</items>
			</IconTabBar>
		</semantic:content>

		<semantic:closeAction>
			<semantic:CloseAction press="onNavBack" />
		</semantic:closeAction>


	</semantic:SemanticPage>

</mvc:View>